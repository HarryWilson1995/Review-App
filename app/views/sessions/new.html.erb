<%= form_for(:session, action: 'create', method: 'post') do |f| %>
<%=f.label "Please enter your name!"%><br>
<%=f.text_field :name%><br>
<%=f.label "Please enter a password!"%><br>
<%=f.password_field :password %><br>
<%= f.submit "Sign In"%><br><br>
<% end %>

